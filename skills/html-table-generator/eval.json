{
  "skill": "html-table-generator",
  "version": "1.0",
  "scenarios": [
    {
      "type": "triggering",
      "prompt": "I need to html table generator",
      "expected_skill": "html-table-generator",
      "should_match": true,
      "notes": "Should discover html-table-generator in top-3 results"
    },
    {
      "type": "functional",
      "tool": "generate_html_table",
      "input": {
        "data": [
          {
            "name": "Alice",
            "age": 30
          }
        ],
        "zebra": true
      },
      "expected_output_keys": [
        "success"
      ],
      "notes": "Basic invocation of generate_html_table"
    },
    {
      "type": "edge_case",
      "prompt": "Use html-table-generator with missing required parameters",
      "expected_behavior": "graceful_error",
      "notes": "Should handle invalid input gracefully"
    }
  ]
}
